{
    "name": "bmatovu/minion",
    "description": "Mirco PHP framework from scratch",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "bmatovu",
            "email": "mtvbrianking@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.4",
        "monolog/monolog": "^1.24",
        "nikic/fast-route": "^1.3",
        "pimple/pimple": "^3.2",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Minion\\": "src/",
            "Slim\\Psr7\\": "packages/slim/Psr7/src"
        }
    },
    "config": {
        "platform": {
          "php": "5.6.4"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
